# Auto detect text files and perform LF normalization
* text=auto

# Rust files
*.rs text eol=lf
*.toml text eol=lf

# Web files
*.js text eol=lf
*.ts text eol=lf
*.json text eol=lf
*.yml text eol=lf
*.yaml text eol=lf
*.md text eol=lf

# Contract artifacts (binary)
*.wasm binary
*.contract binary

# Lock files
Cargo.lock linguist-generated=true
package-lock.json linguist-generated=true

# Documentation
*.md linguist-documentation

# Contract metadata
metadata.json linguist-generated=true
